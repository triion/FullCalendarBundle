{
    "name": "triion/full-calendar-bundle",
    "description": "Symfony4 integration with the library FullCalendar.js",
    "type": "symfony-bundle",
    "keywords": ["calendar", "fullcalendar", "jquery calendar", "symfony4 calendar", "symfony4 bundle"],
    "homepage": "https://github.com/triion/FullCalendarBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Rebeca Mora Anca",
            "email": "anca.rebeca@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "symfony/config": "^3.4|^4.0",
        "symfony/dependency-injection": "^3.4|^4.0",
        "symfony/event-dispatcher": "^3.4|^4.0",
        "symfony/form": "^3.4|^4.0",
        "symfony/http-foundation": "^3.4|^4.0",
        "symfony/http-kernel": "^3.4|^4.0",
        "symfony/property-access": "^3.4|^4.0",
        "symfony/templating": "^3.4|^4.0"
    },
    "require-dev": {
        "phpspec/phpspec": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "AncaRebeca\\FullCalendarBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\AncaRebeca\\FullCalendarBundle\\": "spec"
        }
    }
